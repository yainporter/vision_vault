<%= render partial: "shared/search_form" %>

<% if @search_images %>
  <div id="search-results">
    <% @search_images.each do |image| %>
      <div class="image">
        <%= image_tag url_for(image.vision_image.variant(resize_to_limit: [300, 300])) %>
        <br>
        <%=  image.image_name%>
      </div>
    <% end %>
  </div>
<% end %>
